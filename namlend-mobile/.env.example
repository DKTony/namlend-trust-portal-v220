# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here

# Environment
EXPO_PUBLIC_ENV=development

# API Configuration
EXPO_PUBLIC_API_TIMEOUT=30000

# Feature Flags
EXPO_PUBLIC_ENABLE_BIOMETRIC=true
EXPO_PUBLIC_ENABLE_PUSH_NOTIFICATIONS=true
EXPO_PUBLIC_ENABLE_OFFLINE_MODE=true

# Security
EXPO_PUBLIC_SESSION_TIMEOUT_MINUTES=15
EXPO_PUBLIC_MAX_LOGIN_ATTEMPTS=5

# Namibian Regulatory Compliance
EXPO_PUBLIC_MAX_APR=32
EXPO_PUBLIC_CURRENCY=NAD

# Development Tools (MUST be false in production)
EXPO_PUBLIC_DEBUG_TOOLS=false
